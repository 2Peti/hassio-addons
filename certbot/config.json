{
    "name": "Certbot",
    "version": "0.1.1",
    "slug": "certbot",
    "description": "Certbot compagnion for Nginx Proxy",
    "url": "https://github.com/bestlibre/hassio-addons/tree/master/certbot",
    "startup": "before",
    "boot": "auto",
    "image": "bestlibre/{arch}-certbot",
    "options": {
	"debug": false,
	"email": "email@domain.tld",
	"domains": ["homeassistant.domain.tld"]
    },
    "schema": {
	"debug": "bool",
	"email": "email",
	"domains": ["str"]
    },
    "map": ["ssl:rw"]
}
