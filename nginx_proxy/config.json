{
    "name": "Nginx Proxy",
    "version": "0.1.2",
    "slug": "nginx_proxy",
    "description": "Nginx Proxy for multiple VHOSTS",
    "url": "https://github.com/bestlibre/hassio-addons/tree/master/nginx_proxy",
    "startup": "before",
    "boot": "auto",
    "image": "bestlibre/{arch}-nginx-proxy",
    "options": {"ssl": false, "vhosts":[{"vhost": "homeassistant.domain.tld", "port": "8123", "default_server": true}]},
    "schema": {
	"ssl": "bool",
	"vhosts": [{"vhost": "str", "port": "str", "default_server": "bool"}]
    },
    "ports": {"80/tcp": 80, "443/tcp": 443},
    "map": ["ssl"]
}
